<title>Edit Employee</title>
<h1>Edit Employee</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@employee) do |f| %>

      <%= f.label :role %>
      <%= f.text_field :role %>

      <%= f.label :office_id %>
      <%= f.collection_select :office_id, Office.all, :id, :name %>

      <%= f.submit "Save", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>