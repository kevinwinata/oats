<% provide(:title, 'Company Login') %>
<% provide(:description, 'none') %>
<div>
	<h1>Company Login</h1>
</div>
<div class="login_form">
	<%= form_for(:session, url: "#") do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Login", class: "orange_button login_button" %>
    <% end %>
</div>