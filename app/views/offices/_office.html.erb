<br/>
<%= form_for office do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :office_name %>

    <%= f.label :latitude %>
    <%= f.text_field :office_latitude %>

    <%= f.label :longitude %>
    <%= f.text_field :office_longitude %>

    <%= f.label :range %>
    <%= f.text_field :office_range %>
  </div>
  <%= f.submit "Save", class: "btn btn-large btn-primary" %>
<% end %>
<%= link_to "delete", office, method: :delete, data: { confirm: "You sure?" }, title: office.name %>