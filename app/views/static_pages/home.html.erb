	<h1></h1>
	<div id="home_content">
		<div id="home_content_left">
			<%= image_tag("logo.png", alt: "Oats", id:"logo_home") %>
			<div class="orange_button", onclick="getLocation()">
				<a href="/employee/signin"><h2>Employee Login</h2></a>
			</div>
			<div class="orange_button", id="company_login">
				<a href="/company/signin">Company Login</a>
